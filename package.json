{
  "name": "orbital-ops",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint --fix && vite build",
    "preview": "vite preview",
    "test": "jest",
    "lint": "eslint . --ext ts,tsx,js,jsx && stylelint '**/*.{css,scss,sass,less,styl}'",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix && stylelint '**/*.{css,scss,sass,less,styl}' --fix",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,sass,less,styl}": [
      "stylelint --fix"
    ],
    "*.{ts,tsx}": [
      "tsc --noEmit --skipLibCheck"
    ]
  },
  "dependencies": {
    "@react-three/drei": "9.99.0",
    "@react-three/fiber": "8.15.14",
    "antd": "^6.0.0",
    "clsx": "2.1.0",
    "lucide-react": "0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6",
    "satellite.js": "5.0.0",
    "tailwind-merge": "2.2.0",
    "three": "0.160.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^8.0.0",
    "jest": "^30.2.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8.5.6",
    "stylelint": "^16.26.0",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1",
    "ts-jest": "^29.4.5",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^6.0.2"
  }
}
